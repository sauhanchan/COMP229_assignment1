    <style>
        ul.navbar-nav {
            list-style: none;
            display: inline;
            float:right;
            font-size:18px;
            padding-left: 20px;
        }

        ul.navbar-nav li {
            display: inline-block;
        }        

        a:link,a:visited,a:hover,a:active {
            text-decoration: none;
        }
        nav a:hover,a:active{
            color: white;
        }

        html,body {
        height: 100%;
        margin: 0;
        }

        body {
        display: flex;
        flex-direction: column;
        }

        main {
        flex: 1;
        }
        
        /*#Modify the footer navbar text*/
        footer .navbar-text {
            margin-bottom: 0;
            font-size: small;
        }
        /*#Modify the navbar background and text color */
        .navbar-custom {
            background-color: #095d81;
            color: rgb(255, 255, 255);
            height: 40px;
            padding-top:4px;
            padding-bottom:20px;
        }
        .navbar-custom-top{
            background-color: #095d81;
            color: rgb(255, 255, 255);
        }

        .bgColorJumbo{
            background-color: #def6f8;
        }
